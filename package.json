{
  "name": "analytics-debug-panel",
  "version": "1.0.0",
  "description": "Analytics Debug Panel is an Open Source browser extension to inspect, search & filter analytics traffic in your Developer Console.",
  "author": "Red van Workshop",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "npm run -s lint",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src && echo '\n【ツ】CODE FORMATTING PERFECTION !!!\n'",
    "dev": "NODE_ENV=development webpack --config ./core/webpack.dev.js --hide-modules",
    "build": "NODE_ENV=production webpack --config ./core/webpack.prod.js -p --progress  --hide-modules --colors",
    "pack": "npm run -s pack:crx && npm run -s pack:zip",
    "pack:crx": "crx pack build -o dist/analytics-debug-panel.crx -p key.pem",
    "pack:zip": "cd build && jszip -c ../jszip.json"
  },
  "dependencies": {
    "split.js": "^1.3.5",
    "vue": "^2.5.2",
    "vue-clipboard2": "^0.3.0",
    "vue-json-pretty": "^1.4.1",
    "vue-moment": "^4.0.0",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "archiver": "^2.1.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.1.0",
    "crx": "^4.0.1",
    "css-loader": "^0.28.7",
    "enhanced-resolve": "^3.4.1",
    "eslint": "^4.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue-libs": "^1.2.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "jszip-cli": "^1.4.24",
    "node-sass": "^4.13.1",
    "nodemon": "^1.12.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "sass-loader": "^6.0.6",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.3.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.2",
    "wcer": "^1.0.2",
    "webextension-polyfill": "^0.3.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": ">=3.1.11",
    "webpack-merge": "^4.1.0",
    "ws": "^3.2.0"
  }
}
